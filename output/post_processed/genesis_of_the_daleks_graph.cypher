    ON CREATE SET
        oi.description = 'The Daleks are the ultimate creations that Davros aims to perfect through the outlined chromosomal variations, representing the culmination of his ambition to produce an army devoid of human sentiment.',
        oi.status_before = 'The Daleks exist as prototypes in the incubator, awaiting further genetic modifications dictated by Davros\'s plans.',
        oi.status_after = 'Post-event, the Daleks are primed for their next stage of evolution, potentially leading to a more significant existential threat as their emotional capacities are stripped away.'
    WITH oi
    MATCH (o:Object {uuid: 'object_daleks'})
    MATCH (e:Event {uuid: 'event_6_1'})
