        ap.emotional_tags = ["Harry is a picture of focused intensity, his emotions channeled into unwavering readiness. He is calm", "collected, trusting implicitly in the Doctor's improvised plan. Beneath his stoic exterior lies a deep resolve to protect his friends", "accomplish their mission, fueled by past adventures", "a growing underst", "ing of the stakes."],
        ap.active_plans = ["To remain hidden and undetected until the Doctor initiates the distraction.", "To swiftly and efficiently incapacitate the Thal guards once the Doctor provides an opening.", "To secure the area and ensure the guards pose no further threat."],
        ap.beliefs = ["Loyalty and decisive action are paramount in dangerous situations.", "The Doctor's plans, however unconventional, are usually effective.", "Direct physical intervention is sometimes necessary to overcome immediate threats."],
        ap.goals = ["Short-term: To successfully and silently subdue the Thal guards.", "Medium-term: To assist the Doctor in acquiring the protective gear and proceeding with their mission.", "Ultimate: To support the Doctor in his quest to prevent the Dalek genesis and ensure the safety of Sarah and himself."]
