MERGE (oi:ObjectInvolvement {uuid: 'objectinvolvement_object_time_ring_event_1_3'})
    ON CREATE SET
        oi.description = 'The Time Ring serves as a pivotal tool given to the Doctor by the Time Lord, symbolizing his mission and serving as a means of escape from Skaro once his objectives are completed. It is a physical manifestation of the Time Lord\'s manipulative influence over the Doctor\'s journey.',
        oi.status_before = 'The Time Ring is a new item, yet to be placed on the Doctor\'s wrist, symbolizing potential and the burden of responsibility that comes with it.',
