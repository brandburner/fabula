        ap.active_plans = ["To proclaim the end of the war and celebrate victory with the people.", "To ensure the release of all prisoners and publicly demonstrate generosity.", "To prepare for a victory parade and cement his position as a strong leader."],
        ap.beliefs = ["Victory should be celebrated with mercy and kindness.", "Strong leadership is essential to maintain order after conflict.", "Generosity in victory will foster unity among the Thal people."],
        ap.goals = ["Short-term: To deliver an inspiring speech that rallies the Thal people.", "Medium-term: To showcase power through the release of prisoners which bolsters his reputation.", "Ultimate: To establish a lasting peace and control over the Thal state post-war."]
