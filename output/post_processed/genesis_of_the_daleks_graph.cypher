    MERGE (oi)-[:IN_EVENT]->(e);
MERGE (ap:AgentParticipation {uuid: 'agentparticipation_agent_doctor_event_1_1'})
    ON CREATE SET
        ap.current_status = 'The Doctor appears amidst the chaos of a battleground, attempting to grasp the confusion and danger surrounding him. He stands somewhat isolated, wearing his characteristic attire, with an air of determination and curiosity as he surveys the battlefield and the advancing soldiers.',
        ap.emotional_state = 'The Doctor feels a mix of confusion and concern, driven by the urgency of the situation. Outwardly calm, he displays signs of distress beneath the surface, knowing the implications of the Daleks\' presence and the threat they pose to the universe.',
        ap.emotional_tags = ["The Doctor feels a mix of confusion", "concern, driven by the urgency of the situation. Outwardly calm, he displays signs of distress beneath the surface, knowing the implications of the Daleks' presence", "the threat they pose to the universe."],
        ap.active_plans = ["Assess the battlefield to understand the current situation.", "Engage with the Time Lord to gather information about the Daleks.", "Determine a course of action to avert the impending threat."],
        ap.beliefs = ["The Doctor believes in the sanctity of all life forms and the responsibility to protect them.", "He holds a strong conviction against interfering with time and events, advocating for minimal intervention.", "The Doctor understands the potential darkness within the Daleks and the necessity to counteract their existence."],
        ap.goals = ["Short-term: Gather information and understand the conflicting forces at play.", "Medium-term: Prevent the Daleks from rising to dominance.", "Ultimate: Ensure the safety of all life forms across the universe."]
    ;
MERGE (ap:AgentParticipation {uuid: 'agentparticipation_agent_timelord_event_1_1'})
    ON CREATE SET
        ap.current_status = 'The Time Lord materializes out of the mist, standing with an air of authority. Dressed in flowing robes, he surveys the Doctor with an inquisitive yet superior expression, highlighting the mastery of his time-bending abilities.',
        ap.emotional_state = 'He expresses a calm confidence, but beneath it lies an underlying arrogance. His objective is clear, and he is focused on delivering a crucial message to the Doctor regarding the impending threat of the Daleks.',
        ap.emotional_tags = ["He expresses a calm confidence, but beneath it lies an underlying arrogance. His objective is clear,", "he is focused on delivering a crucial message to the Doctor regarding the impending threat of the Daleks."],
        ap.active_plans = ["Facilitate the Doctor's understanding of the situation.", "Encourage the Doctor to accept the mission regarding the Daleks.", "Reassert the authority of the Time Lords over their former companion."],
        ap.beliefs = ["The Time Lord firmly believes in the superiority of the Time Lords and their right to intervene when necessary.", "He is convinced that knowledge of the future gives him a unique perspective on the present situation.", "He holds a belief that the ends justify the means when it comes to intergalactic matters."],
        ap.goals = ["Short-term: Persuade the Doctor to undertake a mission against the Daleks.", "Medium-term: Ensure the Daleks do not become the dominant species.", "Ultimate: Maintain the supremacy and oversight of the Time Lords over the universe."]
    ;
MERGE (ap:AgentParticipation {uuid: 'agentparticipation_agent_timelord_event_1_2'})
    ON CREATE SET
        ap.current_status = 'The Time Lord stands calmly amidst the desolate battlefield, having materialized to intercept the Doctor\'s transmat. He moves with an air of serene detachment, strolling as if through a garden rather than a war zone. His posture and measured pace underscore his supreme confidence and control over the situation, even in this ravaged landscape.',
        ap.emotional_state = 'The Time Lord projects an aura of effortless superiority and amusement at the Doctor\'s predictable indignation. Beneath this facade of detached amusement, he holds a serious and resolute intent to manipulate the Doctor into accepting a perilous mission. His languid tone and condescending remarks mask a deep-seated concern about the Dalek threat and a calculated strategy to utilize the Doctor\'s unique abilities.',
        ap.emotional_tags = ["The Time Lord projects an aura of effortless superiority", "amusement at the Doctor's predictable indignation. Beneath this facade of detached amusement, he holds a serious", "resolute intent to manipulate the Doctor into accepting a perilous mission. His languid tone", "condescending remarks mask a deep-seated concern about the Dalek threat", "a calculated strategy to utilize the Doctor's unique abilities."],
        ap.active_plans = ["To recruit the Doctor for a mission to Skaro.", "To assert Time Lord authority and technological superiority over the Doctor.", "To downplay the Doctor's objections and emphasize the critical nature of the Dalek threat to ensure his cooperation."],
        ap.beliefs = ["Time Lords possess superior intellect and capabilities compared to other beings.", "Occasional intervention, even manipulation, is justified to maintain the balance of the universe.", "The Doctor, despite his rebellious nature, is ultimately a reliable agent for missions requiring his specific skills and moral compass, however much he may protest."],
        ap.goals = ["Short-term Goal: Secure the Doctor's immediate agreement to undertake the mission on Skaro.", "Medium-term Goal: Ensure the Doctor successfully alters the Daleks' development or discovers a weakness.", "Ultimate Goal: Prevent the Daleks from rising to universal dominance, thereby safeguarding the existing order of the universe as perceived by the Time Lords."]
    ;
MERGE (ap:AgentParticipation {uuid: 'agentparticipation_agent_doctor_event_1_2'})
    ON CREATE SET
        ap.current_status = 'The Doctor materializes abruptly on the war-torn surface of Skaro, initially disoriented and expecting to be elsewhere. He pivots slowly, hands in pockets, his long scarf swirling around him as he takes in the grim surroundings with a puzzled frown. His stance and attire suggest a weary traveler, unprepared for yet another forced detour.',
        ap.emotional_state = 'Initially bewildered and confused by his unexpected arrival, the Doctor quickly transitions to indignation and resentment at the Time Lord\'s interference. He outwardly expresses anger and frustration, challenging the Time Lord’s authority and asserting his hard-won freedom. However, beneath his protests, a flicker of grim curiosity and reluctant sense of duty is ignited by the mention of the Daleks, hinting at his inherent inclination to confront universal threats despite his weariness.',
        ap.emotional_tags = ["Initially bewildered", "confused by his unexpected arrival, the Doctor quickly transitions to indignation", "resentment at the Time Lord's interference. He outwardly expresses anger", "frustration, challenging the Time Lord\u2019s authority", "asserting his hard-won freedom. However, beneath his protests, a flicker of grim curiosity", "reluctant sense of duty is ignited by the mention of the Daleks, hinting at his inherent inclination to confront universal threats despite his weariness."],
        ap.active_plans = ["To ascertain the reasons behind his forced relocation to this desolate battlefield.", "To vehemently protest the Time Lord's interference in his personal freedom and travel plans.", "To explore the situation further upon hearing about the Daleks, despite his initial reluctance, driven by his inherent sense of responsibility."],
        ap.beliefs = ["He has earned his freedom from the Time Lords and their demands after past services.", "Continual interference in his life is an unacceptable violation of his autonomy.", "The Daleks are a significant threat that must be addressed, even if it means reluctantly cooperating with the Time Lords once more."],
        ap.goals = ["Short-term Goal: Re-establish his intended journey and return to the TARDIS with his companions.", "Medium-term Goal: Understand the nature and scope of the Time Lords' request concerning the Daleks.", "Ultimate Goal: Navigate the Time Lords' manipulative tactics while preserving his freedom, yet ultimately address the potential Dalek threat, albeit with initial reluctance and resentment towards the imposed mission."]
    ;
MERGE (ap:AgentParticipation {uuid: 'agentparticipation_agent_doctor_event_1_3'})
    ON CREATE SET
        ap.current_status = 'The Doctor stands amidst the swirling mist of Skaro, engaging in a tense conversation with the Time Lord, displaying a mix of confusion and determination as he contemplates his next move.',
        ap.emotional_state = 'The Doctor exhibits a complex emotional state—an inner turmoil marked by confusion over the Time Lord\'s manipulative actions and resolve to confront the impending threat of the Daleks. His bright blue eyes reflect both a flicker of hope and a burdened past.',
        ap.emotional_tags = ["The Doctor exhibits a complex emotional state\u2014an inner turmoil marked by confusion over the Time Lord's manipulative actions", "resolve to confront the impending threat of the Daleks. His bright blue eyes reflect both a flicker of hope", "a burdened past."],
        ap.active_plans = ["Agree to the Time Lord's request to help with the Dalek situation.", "Prepare to return to Skaro at a crucial point in time.", "Plan how to navigate the dangers of Skaro and confront the Daleks."],
        ap.beliefs = ["His freedom allows him to choose which battles to fight.", "Interference in time can have devastating consequences.", "Every creature, even those who seem evil, has the potential for change."],
        ap.goals = ["Short-term: Successfully complete the mission assigned by the Time Lords.", "Medium-term: Prevent the Daleks from becoming a dominant force.", "Ultimate: Restore peace and balance to the universe by addressing the Dalek threat."]
