        ap.active_plans = ["To initially resist Davros's demands, hoping to stall or find an escape.", "To protect Sarah and Harry from the threatened 'Pain Giver' at all costs.", "To carefully choose his words, attempting to provide information while minimizing the damage to the future."],
        ap.beliefs = ["Compassion and the protection of innocents are paramount, even at personal cost.", "Knowledge, while powerful, must be wielded responsibly and ethically.", "There is always hope for redemption, even in the face of overwhelming darkness, though despair threatens to engulf him."],
        ap.goals = ["{type: short-term, description: To ensure the immediate safety and release of Sarah and Harry from Davros's threat.}", "{type: medium-term, description: To limit the extent of the information Davros gains, mitigating the future impact on the universe.}", "{type: ultimate, description: To find a way to ultimately thwart Davros's plans and prevent the Daleks from achieving universal dominance, despite the compromised situation.}"]
    ;
MERGE (ap:AgentParticipation {uuid: 'agentparticipation_agent_harry_sullivan_event_1_1'})
    ON CREATE SET
