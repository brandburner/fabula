        ap.active_plans = ["To meticulously extract every detail of Dalek defeats from the Doctor's memory.", "To psychologically break the Doctor's resistance and force his cooperation.", "To secure the invaluable information on tape for immediate integration into Dalek programming."],
        ap.beliefs = ["Superior intellect and ruthless efficiency are the only paths to true power.", "Emotional vulnerabilities are weaknesses to be exploited and eliminated.", "The Daleks are the ultimate evolution, and their supremacy is his ordained destiny."],
        ap.goals = ["{type: short-term, description: To compel the Doctor to reveal the secrets of Dalek vulnerabilities in this interrogation session.}", "{type: medium-term, description: To integrate the Doctor's knowledge into the Dalek programming, ensuring their future invincibility.}", "{type: ultimate, description: To secure the Daleks' unchallenged dominion over the universe, fulfilling his vision of ultimate order and control.}"]
