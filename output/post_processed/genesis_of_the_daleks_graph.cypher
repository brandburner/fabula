        ap.goals = ["Short-term: To successfully complete the jump to the nose cone without falling.", "Medium-term: To reach the dome surface and escape the immediate danger of the rocket silo.", "Ultimate: To escape Skaro and return to safety, ideally reuniting with the Doctor and Harry."]
