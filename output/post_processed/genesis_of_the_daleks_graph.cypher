        o.purpose = 'Strategic Objective: To exploit vulnerabilities in the Daleks.',
        o.significance = 'Represents hope and a potential vulnerability in the Dalek threat.'
    ;
MERGE (o:Object {uuid: 'object_elite_troops'})
